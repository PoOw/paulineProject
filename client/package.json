{
  "name": "francis",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.3.1",
    "babel-polyfill": "6.26.0",
    "connected-react-router": "4.5.0",
    "env-cmd": "8.0.2",
    "formik": "1.3.1",
    "history": "4.7.2",
    "react": "16.6.0",
    "react-app-rewire-styled-components": "3.0.2",
    "react-dom": "16.6.0",
    "react-intl": "2.7.2",
    "react-redux": "5.0.7",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.0.5",
    "redux": "4.0.1",
    "redux-persist": "5.10.0",
    "redux-saga": "0.16.2",
    "styled-components": "4.0.2",
    "superagent": "4.0.0-beta.5"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "env-cmd .env.${REACT_APP_ENV} react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "analyze": "source-map-explorer build/static/js/main.*",
    "test:coverage": "npm run test -- --coverage",
    "flow": "flow",
    "flow:coverage": "flow-coverage-report --config=.flowcoverageconfig",
    "generate": "plop --plopfile scripts/generators/index.js",
    "lint": "eslint --ext .jsx,.js -c .eslintrc src",
    "lint:fix": "eslint --fix --ext .jsx,.js -c .eslintrc src"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js",
      "!src/**/actions.js",
      "!src/**/index.js",
      "!src/**/serviceWorker.js",
      "!src/**/*.wrap.js",
      "!src/index.js",
      "!src/tempPolyfills.js",
      "!src/setupTests.js",
      "!src/redux/reducers.js",
      "!src/redux/sagas.js",
      "!src/redux/store.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 0,
        "branches": 0,
        "functions": 0,
        "lines": 0
      }
    }
  },
  "devDependencies": {
    "babel-plugin-styled-components": "1.8.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.6.0",
    "enzyme-to-json": "3.3.4",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jest": "21.26.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-mysticatea": "4.2.4",
    "eslint-plugin-prefer-object-spread": "1.2.1",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "flow-bin": "0.83.0",
    "flow-coverage-report": "0.6.0",
    "flow-typed": "2.5.1",
    "plop": "2.1.0",
    "prettier": "1.14.3",
    "react-app-rewired": "1.6.2",
    "react-test-renderer": "16.6.0",
    "redux-saga-test-plan": "3.7.0",
    "source-map-explorer": "1.6.0"
  }
}
